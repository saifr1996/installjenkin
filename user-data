#!/bin/bash

# Install Git
yum install git -y

# Clone this repository
git clone https://github.com/saifr1996/installjenkin

# Setup permissions on the scripts
cd installjenkin
chmod +x installjava.sh
chmod +x installjenkins.sh
chmod +x installmaven.sh

# Run the scripts
./installjava.sh
./installjenkins.sh
./installmaven.sh


